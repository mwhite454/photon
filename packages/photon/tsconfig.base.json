{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "lib": ["dom", "es2020"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["./node_modules/@types", "./node_modules/dxf-parser-typings"]
  }
}
